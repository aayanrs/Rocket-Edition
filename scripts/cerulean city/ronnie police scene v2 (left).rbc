#dynamic 0x800000
#include stdmoves.rbh
#define RONNIE 0xD

#org @main
lock
spriteface RONNIE FACE_LEFT
spriteface 0xFF FACE_RIGHT
textcolor 0x0
msgbox @ronTalk 0x6
