waitmovement 0x0
sound 0xA
applymovement RONNIE @jump
waitmovement 0x0
pause 0x25
spriteface RONNIE FACE_UP
msgbox @ronYell 0x6
playsong 0x11D 0x0
applymovement RONNIE @ronRun
applymovement 0xFF @playerRun
pause 0x77
sound 0xA
pause 0x8
sound 0xA
waitmovement 0x0
spriteface RONNIE FACE_RIGHT
fadeout 0x0
msgbox @ronTalk4 0x6
pause 0x20
spriteface RONNIE FACE_UP
pause 0x25
spriteface RONNIE FACE_RIGHT
pause 0x15
msgbox @ronTalk5 0x6
applymovement RONNIE @ronLeave1
waitmovement 0x0
sound 0xA
applymovement RONNIE @ronLeave2
waitmovement 0x0
sound 0x9
hidesprite RONNIE
setflag 0x213
setvar 0x401D 0x4
setvar 0x4016 0x3 // re-activates level script in Rocket Warehouse
pause 0x20
playsong 0x134 0x0
release
end

