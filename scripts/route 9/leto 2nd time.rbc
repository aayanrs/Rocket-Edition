msgbox @notEnoughTalk 0x6
pause 0x10
spriteface LETO FACE_RIGHT
pause 0x10
applymovement LETO @noLeaveR
waitmovement 0x0
hidesprite LETO
setflag 0x200
setvar 0x4026 0x2
release
end

#org @acceptR
sound 0x41
paymoney 0x3E8 0x0
updatemoney 0x0 0x0 0x0
checksound
pause 0x8
hidemoney 0x0 0x0
applymovement LETO @stepU
waitmovement 0x0
msgbox @yesTalk 0x6
applymovement LETO @cutStepR
applymovement 0xFF @playerMoveR
waitmovement 0x0
pause 0x15
applymovement LETO @stepLquick
waitmovement 0x0
sound 0x79 // "slash attack"
applymovement TREE @cutting
waitmovement 0x0
hidesprite TREE
setflag 0x21A
checksound
pause 0x20
spriteface LETO FACE_RIGHT
pause 0x10
msgbox @goodBye 0x6
spriteface LETO FACE_DOWN
pause 0x15
applymovement LETO @Dstep
waitmovement 0x0
applymovement LETO @yesLeave
waitmovement 0x0
hidesprite LETO
setflag 0x200
setvar 0x4026 0x2
release
end

#org @rejectR
hidemoney 0x0 0x0
