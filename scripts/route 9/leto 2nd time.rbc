#dynamic 0x800000
#include stdmoves.rbh
#define LETO 0xD
#define TREE 0xA

#org @main
checkflag 0x221
if 0x0 goto @normalT
goto @posCheck
end

#org @normalT
lockall
msgbox 0x81BDFE3 MSG_SIGN
releaseall
end

#org @posCheck
lock
textcolor 0x0
msgbox @talk 0x6
clearflag 0x200
showsprite LETO
getplayerpos 0x8000 0x8001
compare 0x8000 0x3
if 0x1 goto @fromRight
goto @fromLeft
end

#org @fromLeft
pause 0x10
applymovement LETO @approach
waitmovement 0x0
sound 0xA // jump sound
applymovement LETO @jumpLeft
waitmovement 0x0
pause 0x15
spriteface 0xFF FACE_DOWN
spriteface LETO FACE_UP
pause 0x10
msgbox @talk2 0x6
showmoney 0x0 0x0 0x0
msgbox @yesOrNo MSG_YESNO
compare LASTRESULT 0x1
if 0x1 goto @moneyCheckL
goto @rejectL
release
end

#org @moneyCheckL
checkmoney 0x3E8 0x0
compare LASTRESULT 0x1
if 0x1 goto @acceptL
hidemoney 0x0 0x0
msgbox @notEnoughTalk 0x6
pause 0x10
spriteface LETO FACE_LEFT
pause 0x10
applymovement LETO @noLeave1
waitmovement 0x0
sound 0xA // jump sound
applymovement LETO @noLeave2
waitmovement 0x0
hidesprite LETO
setflag 0x200
setvar 0x4026 0x2
release
end

#org @acceptL
sound 0x41
paymoney 0x3E8 0x0
updatemoney 0x0 0x0 0x0
checksound
pause 0x8
hidemoney 0x0 0x0
applymovement LETO @stepU
waitmovement 0x0
msgbox @yesTalk 0x6
applymovement LETO @cutStepL
applymovement 0xFF @playerMoveL
waitmovement 0x0
pause 0x15
applymovement LETO @stepRquick
waitmovement 0x0
sound 0x79 // "slash attack"
applymovement TREE @cutting
waitmovement 0x0
hidesprite TREE
setflag 0x21A
checksound
pause 0x20
spriteface LETO FACE_LEFT
pause 0x10
msgbox @goodBye 0x6
spriteface LETO FACE_RIGHT
pause 0x15
applymovement LETO @yesLeave
waitmovement 0x0
hidesprite LETO
setflag 0x200
setvar 0x4026 0x2
release
end

#org @rejectL
hidemoney 0x0 0x0
msgbox @noTalk 0x6
pause 0x10
spriteface LETO FACE_LEFT
pause 0x10
applymovement LETO @noLeave1
waitmovement 0x0
sound 0xA // jump sound
applymovement LETO @noLeave2
waitmovement 0x0
hidesprite LETO
setflag 0x200
setvar 0x4026 0x2
release
end

#org @fromRight
pause 0x20
applymovement LETO @approach
waitmovement 0x0
pause 0x15
spriteface 0xFF FACE_DOWN
spriteface LETO FACE_UP
pause 0x10
msgbox @talk2 0x6
showmoney 0x0 0x0 0x0
msgbox @yesOrNo MSG_YESNO
compare LASTRESULT 0x1
if 0x1 goto @moneyCheckR
goto @rejectR
release
end

#org @moneyCheckR
checkmoney 0x3E8 0x0
compare LASTRESULT 0x1
if 0x1 goto @acceptR
